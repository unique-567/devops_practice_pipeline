pipeline {
  agent any
  stages {
    stage('plan') {
      steps {
        echo 'i want to plan my application developmment'
      }
    }

    stage('code') {
      steps {
        echo 'development team perform the app development'
      }
    }

    stage('build') {
      steps {
        echo 'Build the application'
      }
    }

    stage('test') {
      steps {
        echo 'testing team perform the test activites'
      }
    }

    stage('deploy') {
      steps {
        echo 'deploy the war files'
      }
    }

    stage('release') {
      steps {
        echo 'release the var files'
      }
    }

    stage('operate') {
      steps {
        echo 'test application'
      }
    }

  }
}